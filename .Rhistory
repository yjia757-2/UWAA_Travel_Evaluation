geom_bar(stat="identity",position="dodge")+
scale_fill_discrete(name="Tour Operators",
breaks=c(1, 2,3,4,5,6),
labels=c("AHI", "GHG","GON","ODY","ORB","SET"))+
xlab("Satisfaction Category")+ylab("Percentage")
